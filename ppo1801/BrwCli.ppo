Static Function nTotImp( oDbfTmp )

   local nRec     := 0
   local nTotImp  := 0

   if !Empty( oDbfTmp ) .AND. ( oDbfTmp:Used() )

      nRec        := oDbfTmp:Recno()

      oDbfTmp:GoTop()
      while !oDbfTmp:Eof()

         nTotImp  += oDbfTmp:nImpDoc

         oDbfTmp:Skip()

      end

      oDbfTmp:GoTo( nRec )

   end

Return ( nTotImp )



