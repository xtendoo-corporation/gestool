                  oDbfTmp:cEstDoc      := "Apartado"
                  oDbfTmp:nUndDoc      := nTotNTikTpv( dbfTikCliL )
                  oDbfTmp:nFacCnv      := ( dbfTikCliL )->nFacCnv

