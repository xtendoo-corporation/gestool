Static Function nTotUnd( oDbfTmp )

   local nRec     := oDbfTmp:Recno()
   local nTotImp  := 0

   oDbfTmp:GoTop()
   while !oDbfTmp:Eof()

      nTotImp     += oDbfTmp:nUndDoc

      oDbfTmp:Skip()

   end

   oDbfTmp:GoTo( nRec )

Return ( nTotImp )



