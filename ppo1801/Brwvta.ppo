               oDbfTmp:cSufDoc         := ( dbfTikCliL )->cSufTil
               oDbfTmp:dFecDoc         := dFecTik( (dbfTikCliL)->cSerTil + (dbfTikCliL)->cNumTil + (dbfTikCliL)->cSufTil, dbfTikCliT )
