static FUNCTION CodigosPostales_OpenFiles( lExclusive, cPath ) ; local Self AS CLASS CodigosPostales := QSelf() AS CLASS CodigosPostales

   local lOpen          := .T.
   local oError
   local oBlock

   If( lExclusive == nil, lExclusive := .F., ) ;
   If( cPath == nil, cPath := ::cPath, ) ;

   oBlock               := ErrorBlock( {| oError | ApoloBreak( oError ) } )
   BEGIN SEQUENCE

      if Empty( ::oDbf )
         ::DefineFiles( cPath )
      end

      ::oDbf:Activate( .F., !( lExclusive ) )

      if !empty( ::oProvincias )
         lOpen          := ::oProvincias:OpenFiles( lExclusive, cPath )
      end

   RECOVER USING oError

      lOpen             := .F.

      msgStop( ErrorMessage( oError ), "Imposible abrir las bases de datos de codigos postales" )

      ::CloseFiles()

   end

   ErrorBlock( oBlock )

RETURN ( lOpen )



static FUNCTION CodigosPostales_CloseFiles( ) ; local Self AS CLASS CodigosPostales := QSelf() AS CLASS CodigosPostales

   if !empty( ::oDbf ) .AND. ( ::oDbf:Used() )
      ::oDbf:End()
   end

   if !empty( ::oProvincias )
      ::oProvincias:End()
   end

   ::oDbf               := nil

RETURN .T.



static FUNCTION CodigosPostales_DefineFiles( cPath, cDriver ) ; local Self AS CLASS CodigosPostales := QSelf() AS CLASS CodigosPostales

   If( cPath == nil, cPath := ::cPath, ) ;
   If( cDriver == nil, cDriver := ::cDriver, ) ;

   ::oDbf := DbfServer( "CODPOSTAL.DBF", "CODPOSTAL" ):New( "CODPOSTAL.DBF", "CODPOSTAL", ( cDriver ), "Codigos postales", ( cPath ) )

      ::oDbf:AddField( "cCodPos", "C", 5, 0,,,,, "Código", .F., 80, .F., {} )
      ::oDbf:AddField( "cNomPos", "C", 60, 0,,,,, "Población", .F., 200, .F., {} )
      ::oDbf:AddField( "cCodPrv", "C", 2, 0,,,,, "Provincia", .F., 200, .F., {} )

      ::oDbf:AddIndex( "cCodPos", "CODPOSTAL.CDX", "cCodPos",,, .F., .F., "Código",,, .T., .F. )
      ::oDbf:AddIndex( "cNomPos", "CODPOSTAL.CDX", "Upper( cNomPos )",,, .F., .F., "Población",,, .T., .F. )



RETURN ( ::oDbf )



