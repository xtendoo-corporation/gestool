   If( cDriver == nil, cDriver := cDriver(), ) ;

   oDbf := DbfServer( "Colaboradores.Dbf", "Colaboraores" ):New( "Colaboradores.Dbf", "Colabora", ( cDriver ), "Colaboradores", ( cPath ) )

      oDbf:AddField( "cCodCol", "C", 3, 0,,,,, "Código", .F., 100, .F., {} )
      oDbf:AddField( "cDesCol", "C", 35, 0,,,,, "Nombre", .F., 400, .F., {} )

      oDbf:AddIndex( "cCodCol", "Colaboradores.Cdx", "cCodCol",,, .F., .F., "Código",,, .T., .F. )
      oDbf:AddIndex( "cDesCol", "Colaboradores.Cdx", "cDesCol",,, .F., .F., "Nombre",,, .T., .F. )



RETURN ( oDbf )



