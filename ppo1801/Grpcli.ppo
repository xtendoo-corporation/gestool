   TButton():ReDefine( 2, {|| oDlg:end() }, oDlg, , , .F. )

   oDlg:bStart             := {|| ::StartTree( nil, oTree ) }

   oDlg:AddFastKey( 116, {|| oDlg:end( 1 ) } )

   oDlg:Activate( , , , .T. )





   if oDlg:nResult == 1
      msgStop( "valor" )
   end

RETURN ( uVal )



static FUNCTION TGrpCli_nombreGrupo( cCodigoGrupo ) ; local Self AS CLASS TGrpCli := QSelf() AS CLASS TGrpCli

   local nombreGrupo := ""

   if ::oDbf:seekinord( cCodigoGrupo, "cCodGrp" )
      nombreGrupo    := ::oDbf:cNomGrp
   end

RETURN ( alltrim( nombreGrupo ) )



function cGruCli( cCodCli, oDbfCli )

   local cCodGrp  := ""

   if oDbfCli:Seek( cCodCli )
      cCodGrp     := oDbfCli:cCodGrp
   end

return( cCodGrp )
