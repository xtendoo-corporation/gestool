   oDbf := DbfServer( "Horas.Dbf", "Horas" ):New( "Horas.Dbf", "Horas", ( cDriver ), "Tipos de horas", ( cPath ) )

      oDbf:AddField( "cCodHra", "C", 3, 0,,,,, "Código", .F., 100, .F., {} )
