      oDbf:AddIndex( "cCodHra", "Horas.Cdx", "cCodHra",,, .F., .F., "Código",,, .T., .F. )
      oDbf:AddIndex( "cDesHra", "Horas.Cdx", "cDesHra",,, .F., .F., "Nombre",,, .T., .F. )



