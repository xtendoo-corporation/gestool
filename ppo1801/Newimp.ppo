      endif

   next

   for n := 1 to len( ::oDbf:aTIndex ) - 1
      if !Empty( ::oDbf:aTIndex[ n ]:cComment )
         aAdd( aOrd, ::oDbf:aTIndex[ n ]:cComment )
      end
