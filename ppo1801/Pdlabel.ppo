     oItem := TLOItem():New(nRow,    bData,    nSize,    aPicture,    oFont,    nPen,    cFmt,    lShadow,    lGrid,    lEan13,    lHorz,    lBanner,    nRgbColor,    Atail(aLabels)[ 1 ] )

