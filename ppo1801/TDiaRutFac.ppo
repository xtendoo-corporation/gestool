static FUNCTION TDiaRutFac_lGenerate( ) ; local Self AS CLASS TDiaRutFac := QSelf() AS CLASS TDiaRutFac

   local aTotal
   local nTotPag
   local cExpHead := ""

   ::oDlg:Disable()
   ::oBtnCancel:Enable()
   ::oDbf:Zap()





   ::aHeader      := {  {|| "Fecha   : " + Dtoc( Date() ) }, {|| "Periodo : " + Dtoc( ::dIniInf ) + " > " + Dtoc( ::dFinInf ) }, {|| "Rutas   : " + if( ::lAllRut, "Todas", AllTrim( ::cRutOrg ) + " > " + AllTrim( ::cRutDes ) ) }, {|| "Estado  : " + ::cEstado }, {|| "Ordenado: " + ::cOrdenado } }

