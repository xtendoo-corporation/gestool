static FUNCTION TInfGrp_IncluyeCero( ) ; local Self AS CLASS TInfGrp := QSelf() AS CLASS TInfGrp





   ::oGruFam:oDbf:GoTop()
   while !::oGruFam:oDbf:Eof()


      if ( ::lAllGrp .OR. ( ::oGruFam:oDbf:cCodGrp >= ::cGruFamOrg .AND. ::oGruFam:oDbf:cCodGrp <= ::cGruFamDes ) ) .AND. !::oDbf:Seek( ::oGruFam:oDbf:cCodGrp )

         ::oDbf:Append()
         ::oDbf:Blank()
         ::oDbf:cGrpFam    := ::oGruFam:oDbf:cCodGrp
         ::oDbf:cNomGrp    := ::oGruFam:oDbf:cNomGrp
         ::oDbf:Save()

      end

      ::oGruFam:oDbf:Skip()

   end

RETURN ( self )



static FUNCTION TInfGrp_IncluyeCeroArt( ) ; local Self AS CLASS TInfGrp := QSelf() AS CLASS TInfGrp

   ::oGruFam:oDbf:GoTop()
   ::oDbfArt:GoTop()

   ::oMtrInf:cText   := "Artículos sin movimiento"
   ::oMtrInf:SetTotal( ::oGruFam:oDbf:OrdKeyCount() )

   while !::oGruFam:oDbf:Eof()

      if ( ::lAllGrp .OR. ( ::oGruFam:oDbf:cCodGrp >= ::cGruFamOrg .AND. ::oGruFam:oDbf:cCodGrp <= ::cGruFamDes ) )

         while !::oDbfArt:Eof()


            if ( ::lAllArt .OR. ( ::oDbfArt:Codigo >= ::cArtOrg .AND. ::oDbfArt:Codigo <= ::cArtDes ) ) .AND. !::oDbf:Seek( ::oGruFam:oDbf:cCodGrp + ::oDbfArt:Codigo )

               ::oDbf:Append()
               ::oDbf:Blank()
               ::oDbf:cGrpFam    := ::oGruFam:oDbf:cCodGrp
               ::oDbf:cNomGrp    := ::oGruFam:oDbf:cNomGrp
               ::oDbf:cCodArt    := ::oDbfArt:Codigo
               ::oDbf:cNomArt    := ::oDbfArt:Nombre
               ::oDbf:Save()

            end

         ::oDbfArt:Skip()

         end

      end

      ::oGruFam:oDbf:Skip()

      ::oMtrInf:AutoInc( ::oGruFam:oDbf:OrdKeyNo() )

      ::oDbfArt:GoTop()

   end

   ::oMtrInf:AutoInc( ::oGruFam:oDbf:LastRec() )

RETURN ( self )



