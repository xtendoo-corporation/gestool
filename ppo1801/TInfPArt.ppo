static FUNCTION TInfPArt_IncluyeCero( ) ; local Self AS CLASS TInfPArt := QSelf() AS CLASS TInfPArt





   ::oDbfArt:GoTop()
   while !::oDbfArt:Eof()


      if ( ::lAllArt .OR. ( ::oDbfArt:codigo >= ::cArtOrg .AND. ::oDbfArt:codigo <= ::cArtDes ) ) .AND. !::oDbf:Seek( ::oDbfArt:codigo )

         ::oDbf:Append()
         ::oDbf:Blank()
         ::oDbf:cCodArt    := ::oDbfArt:codigo
         ::oDbf:cNomArt    := ::oDbfArt:nombre
         ::oDbf:Save()

      end

      ::oDbfArt:Skip()

   end

