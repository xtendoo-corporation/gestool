static FUNCTION TInfTip_IncluyeCero( ) ; local Self AS CLASS TInfTip := QSelf() AS CLASS TInfTip





   ::oTipArt:oDbf:GoTop()
   while !::oTipArt:oDbf:Eof()


      if ( ::lAllTip .OR. ( ::oTipArt:oDbf:cCodTip >= ::cTipOrg   .AND. ::oTipArt:oDbf:cCodTip <= ::cTipDes ) ) .AND. !::oDbf:Seek( ::oTipArt:oDbf:cCodTip )

         ::oDbf:Append()
         ::oDbf:Blank()
         ::oDbf:cCodTip    := ::oTipArt:oDbf:cCodTip
         ::oDbf:cNomTip    := ::oTipArt:oDbf:cNomTip
         ::oDbf:Save()

      end

      ::oTipArt:oDbf:Skip()

   end

RETURN ( self )



static FUNCTION TInfTip_IncluyeCeroArt( ) ; local Self AS CLASS TInfTip := QSelf() AS CLASS TInfTip

   ::oTipArt:oDbf:GoTop()
   ::oDbfArt:GoTop()

   ::oMtrInf:cText               := "Procesando artículos sin movimiento"
   ::oMtrInf:SetTotal( ::oTipArt:oDbf:OrdKeyCount() )

   while !::oTipArt:oDbf:Eof()

      if ( ::lAllTip .OR. ( ::oTipArt:oDbf:cCodTip >= ::cTipOrg .AND. ::oTipArt:oDbf:cCodTip <= ::cTipOrg ) )

         while !::oDbfArt:Eof()


            if ( ::lAllArt .OR. ( ::oDbfArt:Codigo >= ::cArtOrg .AND. ::oDbfArt:Codigo <= ::cArtDes ) ) .AND. !::oDbf:Seek( ::oTipArt:oDbf:cCodTip + ::oDbfArt:Codigo )

               ::oDbf:Append()
               ::oDbf:Blank()
               ::oDbf:cCodTip    := ::oTipArt:oDbf:cCodTip
               ::oDbf:cNomTip    := ::oTipArt:oDbf:cNomTip
               ::oDbf:cCodArt    := ::oDbfArt:Codigo
               ::oDbf:cNomArt    := ::oDbfArt:Nombre
               ::oDbf:Save()

            end

         ::oDbfArt:Skip()

         end

      end

      ::oTipArt:oDbf:Skip()

      ::oMtrInf:AutoInc( ::oTipArt:oDbf:OrdKeyNo() )

      ::oDbfArt:GoTop()

   end

   ::oMtrInf:AutoInc( ::oTipArt:oDbf:LastRec() )

RETURN ( self )



