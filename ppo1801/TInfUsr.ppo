static FUNCTION TInfUsr_IncluyeCero( ) ; local Self AS CLASS TInfUsr := QSelf() AS CLASS TInfUsr



   ::oDbfUsr:GoTop()
   while !::oDbfUsr:Eof()


      if ( ::lAllUsr .OR. ( ::oDbfUsr:cCodUsr >= ::cUsrOrg .AND. ::oDbfUsr:cCodUsr <= ::cUsrDes ) ) .AND. !::oDbf:Seek( ::oDbfUsr:cCodUse )

         ::oDbf:Append()
         ::oDbf:Blank()
         ::oDbf:cCodUsr    := ::oDbfUsr:cCodUse
         ::oDbf:cNomUsr    := ::oDbfUsr:cNbrUse
         ::oDbf:Save()

      end

      ::oDbfUsr:Skip()

   end

