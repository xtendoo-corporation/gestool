                     ::oDbf:cCodGrp       := ::oTikCliL:cGrpFam
                     ::oDbf:cNomGrp       := oRetFld( ::oTikCliL:cGrpFam, ::oGruFam:oDbf )
                     ::oDbf:cCodFam       := ::oTikCliL:cCodFam
                     ::oDbf:cNomFam       := cNomFam( ::oTikCliL:cCodFam, ::oDbfFam )
                     if ::oTikCliT:cTipTik == "4"
