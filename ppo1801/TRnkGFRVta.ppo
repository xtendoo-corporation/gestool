                  ::oDbf:nTotUni    := -( nTotNFacRec( ::oFacRecL ) )
                  ::oDbf:nTotNet    := -( nImpLFacRec( ::oFacRecT:cAlias, ::oFacRecL:cAlias, ::nDecOut, ::nDerOut, ::nValDiv ) )
