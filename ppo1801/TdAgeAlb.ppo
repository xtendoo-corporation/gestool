   ::aHeader      := {  {|| "Fecha   : " + Dtoc( Date() ) }, {|| "Periodo : " + Dtoc( ::dIniInf ) + " > " + Dtoc( ::dFinInf ) }, {|| "Agentes : " + ::cAgeOrg         + " > " + ::cAgeDes }, {|| "Estado  : " + ::cEstado }, {|| "Ordenado: " + ::cOrdenado } }





