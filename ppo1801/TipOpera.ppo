   If( cDriver == nil, cDriver := cDriver(), ) ;

   oDbf := DbfServer( "TipOpera.Dbf", "TipOpera" ):New( "TipOpera.Dbf", "TipOpera", ( cDriver ), "Tipos de operaciones", ( cPath ) )

      oDbf:AddField( "cCodTip", "C", 3, 0,,,,, "Código", .F., 100, .F., {} )
      oDbf:AddField( "cDesTip", "C", 35, 0,,,,, "Nombre", .F., 400, .F., {} )

      oDbf:AddIndex( "cCodTip", "TipOpera.Cdx", "cCodTip",,, .F., .F., "Código",,, .T., .F. )
      oDbf:AddIndex( "cDesTip", "TipOpera.Cdx", "cDesTip",,, .F., .F., "Nombre",,, .T., .F. )



RETURN ( oDbf )



