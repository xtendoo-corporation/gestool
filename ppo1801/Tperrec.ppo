      case ::oEstado:nAt == 2
         bValid   := {|| !::oFacCliP:lCobrado }
      case ::oEstado:nAt == 3
         bValid   := {|| .T. }
   end









   ::aHeader   := {  {|| "Fecha   : " + Dtoc( Date() ) }, {|| "Periodo : " + Dtoc( ::dIniInf ) + " > " + Dtoc( ::dFinInf ) }, {|| "Cliente : " + AllTrim( ::cCliOrg ) + " > " + AllTrim( ::cCliDes ) }, {|| "Estado  : " + ::cEstado }, {|| "Dias    : " + if( ::lNumDias, Trans( ::nNumDias, "999" ), "Sin especificar" ) } }

