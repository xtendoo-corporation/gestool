static FUNCTION TTotalTurno_addImporte( cCaja, cSerie, nImporte, aImporte, cData, aData, bEdit, nBmp ) ; local Self AS CLASS TTotalTurno := QSelf() AS CLASS TTotalTurno

   local n

   If( nBmp == nil, nBmp := 0, ) ;

   if ( n := aScan( aImporte, {|a| a[ 1 ] == cCaja .AND. a[ 2 ] == cSerie } ) ) <> 0
      aImporte[ n, 3 ]  += nImporte
   else
      aAdd( aImporte, { cCaja, cSerie, nImporte } )
   end

   if !Empty( cData )
      aAdd( aData, { cCaja, cSerie, cData, nImporte, bEdit, nBmp } )
   end

RETURN ( Self )



static FUNCTION TTotalTurno_addNumero( cCaja, aData ) ; local Self AS CLASS TTotalTurno := QSelf() AS CLASS TTotalTurno

   local n

   if ( n := aScan( aData, {|a| a[ 1 ] == cCaja } ) ) <> 0
      aData[ n, 2 ]++
   else
      aAdd( aData, { cCaja, 1 } )
   end

RETURN ( Self )































static FUNCTION TTotalTurno_nTotVentas( cCaja, cSerie ) ; local Self AS CLASS TTotalTurno := QSelf() AS CLASS TTotalTurno

